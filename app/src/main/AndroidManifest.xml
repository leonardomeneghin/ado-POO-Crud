<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pooexercise">


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_img_dog_drawer"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_img_dog_drawer"
        android:supportsRtl="true"
        android:theme="@style/Theme.pooexercise"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activitys.TermosUso"
            android:exported="false" />
        <activity
            android:name=".activitys.PerfilUsuario"
