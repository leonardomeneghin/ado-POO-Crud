            android:exported="false" />
        <activity
            android:name=".activitys.PerfilAnimal"
            android:exported="false" />
        <activity
            android:name=".activitys.PerfilInstituicao"
            android:exported="false" />
        <activity
            android:name=".activitys.CadastroIntegrante"
            android:exported="false" />
        <activity
            android:name=".activitys.CadastrarInstituicao"
            android:exported="false" />
        <activity
            android:name=".activitys.CadastroAnimal"
            android:exported="false" />
        <activity
            android:name=".activitys.Login"
            android:exported="false" />
        <activity
            android:name=".activitys.MainActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>